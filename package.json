{
  "name": "n8n-nodes-okta",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Okta identity management, providing 10 resources and 80+ operations for user lifecycle, groups, applications, policies, MFA, and system administration.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-okta.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-okta#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-okta/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "okta",
    "identity",
    "iam",
    "sso",
    "mfa",
    "authentication",
    "user-management",
    "directory",
    "oauth",
    "saml"
  ],
  "main": "dist/nodes/Okta/Okta.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OktaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Okta/Okta.node.js",
      "dist/nodes/Okta/OktaTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.26.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
